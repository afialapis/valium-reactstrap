{
  "name": "valium-reactstrap",
  "version": "0.0.76",
  "description": "A Valium implementation for Reactstrap",
  "author": "Donato Lorenzo <donato@afialapis.com>",
  "contributors": [
    "Donato Lorenzo <donato@afialapis.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afialapis/valium-reactstrap.git"
  },
  "bugs": {
    "url": "https://github.com/afialapis/valium-reactstrap/issues"
  },
  "homepage": "https://valium-reactstrap.afialapis.com/",
  "files": [
    "lib",
    "dist"
  ],
  "main": "dist/valium-reactstrap.js",
  "browser": "dist/valium-reactstrap.umd.js",
  "module": "dist/valium-reactstrap.es.js",
  "scripts": {
    "o_lint": "eslint src",
    "o_test": "karma start test/karma.conf.js",
    "o_test-run": "karma start test/karma.conf.js --no-single-run",
    "o_clean-lib": "rimraf lib && mkdir lib",
    "o_build-lib-css": "node_modules/node-sass/bin/node-sass src/assets/style.scss lib/style.css",
    "o_build-lib-js": "babel src -d lib",
    "o_lib": "npm run clean-lib && npm run build-lib-js && npm run build-lib-css",
    "o_clean-dist": "rimraf dist && mkdir dist",
    "o_build-dist-css": "node_modules/node-sass/bin/node-sass src/assets/style.scss dist/style.css",
    "o_build-dist-css-min": "node_modules/node-sass/bin/node-sass src/assets/style.scss dist/style.min.css --output-style compressed",
    "o_build-dist-js": "webpack",
    "o_build-dist-js-min": "NODE_ENV=production webpack",
    "o_dist": "npm run clean-dist && npm run build-dist-js && npm run build-dist-js-min && npm run build-dist-css && npm run build-dist-css-min",
    "o_clean": "npm run clean-lib && npm run clean-dist",
    "o_prepublish": "npm run test && npm run lib && npm run dist",
    "o_demo": "NODE_ENV=development node demo/server",
    "clean-test": "rm -f test/bundle.js",
    "clean-lib": "rm -rf lib && mkdir lib",
    "clean-dist": "rm -rf dist && mkdir dist",
    "lint": "eslint src",
    "test": "npm run clean-test && rollup -c rollup.test.js && karma start --singleRun",
    "lib": "npm run clean-lib && babel src -d lib",
    "dist": "rollup -c",
    "prepare": "npm run test && npm run lib && npm run dist",
    "demo": "rm -rf demo/dist && mkdir demo/dist && rollup -c rollup.demo.js -w"
  },
  "keywords": [
    "js",
    "react",
    "form"
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.2",
    "co": "^4.6.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.6.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.10.0",
    "mocha": "^8.1.1",
    "node-sass": "^4.14.1",
    "node-uuid": "^1.4.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-terser": "^7.0.0",
    "sass-loader": "^9.0.3",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "reactstrap-date-picker": "0.0.9",
    "valium": "0.0.32"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "reactstrap": "^8.5.1",
    "prop-types": ">=15.7.2"
  }
}
