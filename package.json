{
  "name": "valium-reactstrap",
  "version": "0.0.31",
  "description": "A Valium implementation for Reactstrap",
  "author": "Donato Lorenzo <donato@afialapis.com>",
  "contributors": [
    "Donato Lorenzo <donato@afialapis.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afialapis/valium-reactstrap.git"
  },
  "bugs": {
    "url": "https://github.com/afialapis/valium-reactstrap/issues"
  },
  "homepage": "https://valium-reactstrap.afialapis.com/",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src",
    "test": "karma start test/karma.conf.js",
    "test-run": "karma start test/karma.conf.js --no-single-run",
    "clean-lib": "rimraf lib && mkdir lib",
    "build-lib-css": "node_modules/node-sass/bin/node-sass src/assets/style.scss lib/style.css",
    "build-lib-js": "babel src -d lib",
    "lib": "npm run clean-lib && npm run build-lib-js && npm run build-lib-css",
    "clean-dist": "rimraf dist && mkdir dist",
    "build-dist-css": "node_modules/node-sass/bin/node-sass src/assets/style.scss dist/style.css",
    "build-dist-css-min": "node_modules/node-sass/bin/node-sass src/assets/style.scss dist/style.min.css --output-style compressed",
    "build-dist-js": "webpack",
    "build-dist-js-min": "NODE_ENV=production webpack",
    "dist": "npm run clean-dist && npm run build-dist-js && npm run build-dist-js-min && npm run build-dist-css && npm run build-dist-css-min",
    "clean": "npm run clean-lib && npm run clean-dist",
    "prepublish": "npm run test && npm run lib && npm run dist",
    "demo": "NODE_ENV=development node demo/server"
  },
  "keywords": [
    "js",
    "react",
    "form"
  ],
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "css-loader": "^3.2.0",
    "co": "^4.6.0",
    "eslint": "^6.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.2.0",
    "node-sass": "^4.12.0",
    "node-uuid": "^1.4.8",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^2.0.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reactstrap": "^8.0.1",
    "reactstrap-date-picker": "0.0.7",
    "valium": "0.0.26"
  }
}
